{
  "name": "styled-style",
  "version": "0.6.0",
  "description": "css modules like styled-components",
  "license": "MIT",
  "repository": "akameco/styled-style",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "main": "lib/index.js",
  "jsnext:main": "lib/index.esm.js",
  "module": "lib/index.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint --fix .",
    "test": "jest",
    "prepare": "yarn build && yarn build:flow",
    "build": "yarn rollup && yarn build:flow",
    "rollup": "rollup -c",
    "build:lib": "babel src -d lib --ignore '**/*.test.js'",
    "build:flow": "flow-copy-source -v -i '*.test.js' src lib",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "react",
    "styled-components",
    "styled",
    "css",
    "css-moduels",
    "css-module"
  ],
  "dependencies": {
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.4.16",
    "all-contributors-cli": "^5.4.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "5.6.1",
    "eslint-config-zero": "^0.3.0",
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^1.3.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/jestSetup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
